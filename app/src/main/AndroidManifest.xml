<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.babsangapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data android:name="android.max_aspect" android:value="2.1" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".BasicActivity" android:parentActivityName=".MainActivity">
        </activity>

        <activity android:name=".HealthyActivity" android:parentActivityName=".MainActivity">
        </activity>

        <activity android:name=".BiggerActivity" android:parentActivityName=".MainActivity">
        </activity>

        <activity android:name="SoloActivity" android:parentActivityName=".MainActivity">
        </activity>


    </application>

</manifest>